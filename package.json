{
  "name": "wodensdag",
  "version": "0.4.0",
  "license": "CC0-1.0",
  "description": "Yesterday's shows",
  "repository": "http://github.com/dlq/wodensdag",
  "author": "Darcy Quesnel <darcy.quesnel@nonaverse.com>",
  "main": "main.js",
  "keywords": [
    "electron",
    "desktop",
    "tv",
    "schedule",
    "torrents"
  ],
  "scripts": {
    "start": "electron --inspect .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --mac --win"
  },
  "build": {
    "appId": "com.nonaverse.wodensdag",
    "mac": {
      "category": "public.app-category.entertainment"
    },
    "publish": [
      {
        "provider": "github"
      }
    ]
  },
  "devDependencies": {
    "electron": "^2.0.8",
    "electron-builder": "^20.28.4",
    "eslint": "^5.5.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-standard": "^4.0.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.3.1",
    "bootstrap": "^4.1.3",
    "electron-window-state": "^5.0.1",
    "handlebars": "^4.0.12",
    "jquery": "^3.3.1",
    "moment": "^2.22.2",
    "popper.js": "^1.14.4",
    "torrent-search-api": "^2.0.3"
  }
}
